{
  "name": "@mansagroup/rabbitmq-fp",
  "version": "0.0.6",
  "description": "A straightforward RabbitMQ client with fp-ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^16 || ^18"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:js & npm run build:types",
    "build:js": "swc ./src -d dist",
    "build:types": "tsc --build ./tsconfig.build.json",
    "lint": "eslint \"{src,test}/**/*.{js,ts}\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "prepare": "husky install",
    "prerelease": "npm run build",
    "release": "np --message=\"chore: v%s\"",
    "depcheck": "depcheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MansaGroup/rabbitmq-fp.git"
  },
  "keywords": [
    "rabbitmq",
    "client",
    "functionnal",
    "fp-ts",
    "amqp"
  ],
  "author": "Jérémy Levilain <jeremy.levilain@getmansa.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MansaGroup/rabbitmq-fp/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "header-max-length": [
        2,
        "always",
        85
      ]
    }
  },
  "homepage": "https://github.com/MansaGroup/rabbitmq-fp#readme",
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@mansagroup/eslint-config": "0.0.2-4",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.197",
    "@swc/jest": "0.2.22",
    "@types/amqplib": "0.8.2",
    "@types/jest": "28.1.6",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "axios": "0.27.2",
    "depcheck": "1.4.3",
    "eslint": "8.20.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-extended": "3.0.1",
    "jest-sonar": "0.2.12",
    "lint-staged": "13.0.3",
    "np": "7.6.2",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "testcontainers": "8.12.0",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4",
    "wait-for-expect": "3.0.2"
  },
  "peerDependencies": {
    "fp-ts": ">=2"
  },
  "dependencies": {
    "amqp-connection-manager": "4.1.3",
    "amqplib": "0.10.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
